<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.bluemoonmanagement.controllers.Resident_management.DeleteOwner_Controller">
    <VBox alignment="CENTER" spacing="10" prefWidth="300" prefHeight="150">
        <Label fx:id="messageLabel" text="Are you sure you want to delete this entry?" wrapText="true"/>
        <HBox alignment="CENTER" spacing="10">
            <Button fx:id="confirmButton" text="Yes" onAction="#handleConfirm"/>
            <Button fx:id="cancelButton" text="No" onAction="#handleCancel"/>
        </HBox>
    </VBox>
</AnchorPane>
