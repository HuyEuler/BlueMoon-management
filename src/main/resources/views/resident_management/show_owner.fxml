<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.bluemoonmanagement.controllers.Resident_management.showOwner_controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="900.0" minWidth="900.0" prefWidth="900.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="100.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <TableView fx:id="tableView" prefHeight="434.0" prefWidth="864.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
      <columns>
          <TableColumn fx:id="cHoTen" prefWidth="265.0" text="Họ và tên" />
          <TableColumn fx:id="cNgaySinh" prefWidth="232.0" text="Ngày sinh" />
          <TableColumn fx:id="cSDT" prefWidth="205.0" text="Số điện thoại" />
          <TableColumn fx:id="cMQH" prefWidth="197.0" text="Mối quan hệ với chủ hộ" />
      </columns>
    </TableView>
    <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #809bce;" />
    <Label fx:id="lbNumberRoom" text="Danh sách cư dân phòng" textFill="WHITE" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
        <font>
          <Font name="System Bold" size="33.0" />
        </font>
    </Label>
</GridPane>
