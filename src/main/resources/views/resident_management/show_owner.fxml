<GridPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bluemoonmanagement.controllers.Resident_management.showOwner_controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="900.0" minWidth="900.0" prefWidth="900.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="100.0" minHeight="50.0" prefHeight="70.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="500.0" minHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <TableView prefHeight="434.0" prefWidth="864.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
      <columns>
          <TableColumn prefWidth="265.0" text="Họ và tên" />
          <TableColumn prefWidth="232.0" text="Ngày sinh" />
          <TableColumn prefWidth="205.0" text="Số điện thoại" />
          <TableColumn prefWidth="197.0" text="Mối quan hệ với chủ hộ" />
      </columns>
    </TableView>
    <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #809bce;" />
    <Label fx:id="numberRoom" text="Danh sách cư dân phòng" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
        <font>
          <Font name="System Bold" size="33.0" />
        </font>
