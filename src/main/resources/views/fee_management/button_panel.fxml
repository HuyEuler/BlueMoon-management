<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane minHeight="500.0" prefHeight="650.0" prefWidth="200.0" xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.bluemoonmanagement.controllers.Fee_management.FeeManagementController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="150.0" prefWidth="200.0"/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="80.0" prefHeight="130.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="80.0" prefHeight="130.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="80.0" prefHeight="130.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="80.0" prefHeight="130.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="80.0" prefHeight="130.0" vgrow="SOMETIMES"/>
   </rowConstraints>
   <Button fx:id="addFee" alignment="BOTTOM_CENTER" contentDisplay="TOP" minHeight="80.0" mnemonicParsing="false"
           prefHeight="100.0" prefWidth="150.0" styleClass="panel_button" stylesheets="@../views_styles.css"
           text="Thêm loại phí" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="CENTER"
           GridPane.valignment="TOP">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/addFee.png"/>
         </ImageView>
      </graphic>
      <GridPane.margin>
         <Insets/>
      </GridPane.margin>
   </Button>
   <Button fx:id="listPaid" alignment="BOTTOM_CENTER" contentDisplay="TOP" minHeight="80.0" mnemonicParsing="false"
           prefHeight="100.0" prefWidth="150.0" styleClass="panel_button" stylesheets="@../views_styles.css"
           text="Danh sách đã nộp" textFill="WHITE" GridPane.halignment="CENTER" GridPane.rowIndex="8"
           GridPane.valignment="BOTTOM">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/payList.png"/>
         </ImageView>
      </graphic>
      <GridPane.margin>
         <Insets/>
      </GridPane.margin>
   </Button>
   <Button fx:id="payFee" alignment="BOTTOM_CENTER" contentDisplay="TOP" minHeight="80.0" mnemonicParsing="false"
           prefHeight="100.0" prefWidth="150.0" styleClass="panel_button" stylesheets="@../views_styles.css"
           text="Nộp phí" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="CENTER" GridPane.rowIndex="6"
           GridPane.valignment="CENTER">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="82.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/pay.png"/>
         </ImageView>
      </graphic>
      <GridPane.margin>
         <Insets/>
      </GridPane.margin>
   </Button>
   <Button fx:id="deleteFee" alignment="BOTTOM_CENTER" contentDisplay="TOP" minHeight="80.0" mnemonicParsing="false"
           prefHeight="100.0" prefWidth="150.0" styleClass="panel_button" stylesheets="@../views_styles.css"
           text="Xóa loại phí" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="CENTER"
           GridPane.rowIndex="4" GridPane.valignment="CENTER">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/deleteFee.png"/>
         </ImageView>
      </graphic>
      <GridPane.margin>
         <Insets/>
      </GridPane.margin>
   </Button>
   <Button fx:id="editFee" alignment="BOTTOM_CENTER" contentDisplay="TOP" minHeight="80.0" mnemonicParsing="false"
           prefHeight="100.0" prefWidth="150.0" styleClass="panel_button" stylesheets="@../views_styles.css"
           text="Sửa loại phí" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="CENTER"
           GridPane.rowIndex="2" GridPane.valignment="CENTER">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
      <graphic>
         <ImageView fitHeight="50.0" fitWidth="82.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../images/editFee.png"/>
         </ImageView>
      </graphic>
      <GridPane.margin>
         <Insets/>
      </GridPane.margin>
   </Button>
</GridPane>
